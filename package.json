{
  "name": "react-redux-flipper",
  "version": "0.0.8",
  "description": "React Flipper Component",
  "author": {
    "name": "Rostyslav Khanas",
    "email": "rk@try.cards"
  },
  "repository": {
    "type": "git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src",
    "dev": "cd examples/main && NODE_ENV=development webpack",
    "prod": "cd examples/main && NODE_ENV=production webpack -p --config",
    "build": "npm run lint && npm run clean && babel src --out-dir lib",
    "build:all": "npm run build && npm run dev",
    "start": "cd examples/main && webpack-dev-server --inline --hot"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {},
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "rimraf": "^2.3.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}
