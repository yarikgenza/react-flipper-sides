{
  "name": "react-redux-flipper",
  "version": "0.0.1-rc.3",
  "description": "React Flipper Component",
  "author": {
    "name": "Rostyslav Khanas",
    "email": "rk@try.cards"
  },
  "repository": {
    "type": "git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src",
    "dev": "cd examples/main && NODE_ENV=development webpack",
    "prod": "cd examples/main && NODE_ENV=production webpack -p --config",
    "build": "npm run lint && npm run clean && babel src --out-dir lib",
    "build:all": "npm run build && npm run dev",
    "start": "cd examples/main && webpack-dev-server --inline --hot"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "exception-methods": "^0.0.9",
    "react": "^15.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.1"
  },
  "devDependencies": {
    "autobind-decorator": "^1.3.3",
    "babel": "^6.3.26",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "rimraf": "^2.3.4",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
